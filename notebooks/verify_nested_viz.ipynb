
import hypernodes as hn
from hypernodes import Pipeline

# Define some simple nodes
@hn.node
def add(a: int, b: int) -> int:
    return a + b

@hn.node
def multiply(a: int, b: int) -> int:
    return a * b

@hn.node
def square(x: int) -> int:
    return x * x

# Create a sub-pipeline
sub_pipe = Pipeline([
    add(a=hn.input("x"), b=hn.input("y")),
    square(x=add)
]).bind(y=10)

# Wrap it as a node
sub_node = sub_pipe.as_node(
    input_mapping={
    "x": "outer_a"
},
    output_mapping={
    "square": "result"
},
    name="MathSubPipeline"
)

# Create main pipeline
main_pipe = Pipeline([
    multiply(a=hn.input("start"), b=2),
    sub_node(outer_a=multiply),
    add(a=sub_node, b=5)
])

# Visualize
widget = main_pipe.visualize(interactive=True)
widget
